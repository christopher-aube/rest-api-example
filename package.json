{
  "name": "rest-api-example",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:site": "parcel serve --target site --no-cache --port 5001 --open",
    "start:server": "parcel watch --target server --port 5002 --no-cache",
    "reload:backend": "wait-on dist/server.js && nodemon --watch dist/server.js --exec node dist/server.js",
    "prestart": "rimraf dist && rimraf ../.parcel-cache",
    "start": "cross-env SERVER_PORT=5003 concurrently \"yarn run start:site\" \"yarn run start:server\" \"yarn run reload:backend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopher-aube/rest-api-example.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christopher-aube/rest-api-example/issues"
  },
  "homepage": "https://github.com/christopher-aube/rest-api-example#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "14.18",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^8.3.4",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.5.0",
    "config-ts-react": "git+https://git@github.com/christopher-aube/config-ts-react.git#v0.0.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.26.0",
    "events": "^3.3.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.18",
    "postcss-modules": "^4.3.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "typescript": "^4.8.4",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "uuid": "^9.0.0"
  },
  "eslintConfig": {
    "extends": "config-ts-react"
  },
  "prettier": "config-ts-react/prettier",
  "stylelint": {
    "extends": "config-ts-react/stylelint"
  },
  "site": "./dist/site.html",
  "server": "./dist/server.js",
  "targets": {
    "site": {
      "source": "./src/site/index.html",
      "distDir": "./dist"
    },
    "server": {
      "source": "./src/server/index.ts",
      "context": "node",
      "distDir": "./dist"
    }
  }
}
